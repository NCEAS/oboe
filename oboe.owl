<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl11 "http://www.w3.org/2006/12/owl11#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl11xml "http://www.w3.org/2006/12/owl11-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY oboe "http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#" >
]>


<rdf:RDF xmlns="http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#"
     xml:base="http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl"
     xmlns:owl11="http://www.w3.org/2006/12/owl11#"
     xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:oboe="http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <owl:versionInfo rdf:datatype="&xsd;string"
            >&apos;$Id: Observation.owl,v 1.29 2008/01/30 04:36:12 bowers Exp $&apos;</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#characteristicOf -->

    <owl:ObjectProperty rdf:about="#characteristicOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <owl:inverseOf rdf:resource="#hasCharacteristic"/>
        <rdfs:comment
            >This property relates a characteristic to the entity it is a characteristic of. A characteristic is of exactly one entity.</rdfs:comment>
        <rdfs:range rdf:resource="#Entity"/>
        <rdfs:domain rdf:resource="#Characteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#forCharacteristic -->

    <owl:ObjectProperty rdf:about="#forCharacteristic">
        <rdfs:domain rdf:resource="#MeasurementStandard"/>
        <rdfs:range rdf:resource="#Characteristic"/>
        <rdfs:comment
            >This property relates measurement standards to the characteristics they are standards for.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasBaseDimension -->

    <owl:ObjectProperty rdf:about="#hasBaseDimension">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#SimpleDerivedDimension"/>
        <rdfs:range rdf:resource="#BaseDimension"/>
        <rdfs:comment
            >This property gives the base dimension of a simple derived dimension.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasBaseUnit -->

    <owl:ObjectProperty rdf:about="#hasBaseUnit">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#BaseUnit"/>
        <rdfs:domain rdf:resource="#SimpleDerivedUnit"/>
        <rdfs:comment
            >This property gives the base unit of a simple derived unit.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasCharacteristic -->

    <owl:ObjectProperty rdf:about="#hasCharacteristic">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="#Characteristic"/>
        <rdfs:comment
            >This property relates an entity to one of its characteristics.</rdfs:comment>
        <rdfs:domain rdf:resource="#Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasCharacteristicValue -->

    <owl:ObjectProperty rdf:about="#hasCharacteristicValue">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >This property gives the value of an entity&apos;s characteristic. Characteristics have exactly one value.</rdfs:comment>
        <rdfs:range rdf:resource="#CharacteristicValue"/>
        <rdfs:domain rdf:resource="#Characteristic"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasContext -->

    <owl:ObjectProperty rdf:about="#hasContext">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="#Observation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property states that one observation serves as the context for another observation.  Context defines a semantic relationship between two entities that is a fundamental aspect of the observations, but not necessarily of the entities themselves.  For example, most measurements are accomplished in a spatio-temporal framework that might be valuable context. The assertions made by contextual observations are assumed of the contextualized observations. Context is a transitive relationship.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasContextObservation -->

    <owl:ObjectProperty rdf:about="#hasContextObservation">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Observation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property connects a context object to an observation, where the entity of the observation is serving as context for some other observed entity.</rdfs:comment>
        <rdfs:domain rdf:resource="#Context"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasContextRelationship -->

    <owl:ObjectProperty rdf:about="#hasContextRelationship">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property relates a context object to its associated relationship object.</rdfs:comment>
        <rdfs:range rdf:resource="#Relationship"/>
        <rdfs:domain rdf:resource="#Context"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasDimension -->

    <owl:ObjectProperty rdf:about="#hasDimension">
        <rdfs:domain rdf:resource="#ComplexDerivedDimension"/>
        <rdfs:comment
            >This property relates a complex derived dimension to one of its component dimensions.</rdfs:comment>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#BaseDimension"/>
                    <rdf:Description rdf:about="#SimpleDerivedDimension"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasEntityRelationship -->

    <owl:ObjectProperty rdf:about="#hasEntityRelationship">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property connects an entity to a relationship object, where the entity plays the role of the source of the relationship.</rdfs:comment>
        <rdfs:range rdf:resource="#Relationship"/>
        <rdfs:domain rdf:resource="#Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasHigherRank -->

    <owl:ObjectProperty rdf:about="#hasHigherRank">
        <rdf:type rdf:resource="&owl11;IrreflexiveProperty"/>
        <rdf:type rdf:resource="&owl11;AntisymmetricProperty"/>
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:comment
            >This property represents an ordering (precedence) relationship between two characteristic values. The higher rank property induces a strict (irreflexive, asymmetric) partial ordering over a given set of characteristic values.</rdfs:comment>
        <rdfs:domain rdf:resource="#OrderedValue"/>
        <rdfs:range rdf:resource="#OrderedValue"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasMeasurement -->

    <owl:ObjectProperty rdf:about="#hasMeasurement">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="#Measurement"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property gives the measurements of the observed entity.</rdfs:comment>
        <rdfs:domain rdf:resource="#Observation"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasObservationContext -->

    <owl:ObjectProperty rdf:about="#hasObservationContext">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="#Observation"/>
        <rdfs:range rdf:resource="#Context"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property connects an observation to a context object, where the entity of the observation is being contextualized.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasRelationshipEntity -->

    <owl:ObjectProperty rdf:about="#hasRelationshipEntity">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Relationship"/>
        <rdfs:range rdf:resource="#Entity"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property connects a relationship object to an entity,  where the entity plays the role of the target of the relationship.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasSourceUnit -->

    <owl:ObjectProperty rdf:about="#hasSourceUnit">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#BaseUnit"/>
        <rdfs:domain rdf:resource="#UnitConversion"/>
        <rdfs:comment
            >This property gives the source base unit of a unit conoboe. The source unit is the unit being mapped from.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasStandardValue -->

    <owl:ObjectProperty rdf:about="#hasStandardValue">
        <rdfs:range rdf:resource="#StandardValue"/>
        <rdfs:comment
            >This property gives the standard values of a measurement standard.</rdfs:comment>
        <rdfs:domain rdf:resource="#MeasurementStandard"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasTargetUnit -->

    <owl:ObjectProperty rdf:about="#hasTargetUnit">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#BaseUnit"/>
        <rdfs:comment
            >This property gives the target base unit of a unit conoboe. The target unit is the unit being mapped to.</rdfs:comment>
        <rdfs:domain rdf:resource="#UnitConversion"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasUnit -->

    <owl:ObjectProperty rdf:about="#hasUnit">
        <rdfs:domain rdf:resource="#ComplexDerivedUnit"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property relates a complex derived unit with one of its component units.</rdfs:comment>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#SimpleDerivedUnit"/>
                    <rdf:Description rdf:about="#BaseUnit"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#ofCharacteristic -->

    <owl:ObjectProperty rdf:about="#ofCharacteristic">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Characteristic"/>
        <rdfs:domain rdf:resource="#Measurement"/>
        <rdfs:comment
            >This property gives the entity characteristic measured by the measurement.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#ofEntity -->

    <owl:ObjectProperty rdf:about="#ofEntity">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Entity"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property gives the observed entity of an observation.</rdfs:comment>
        <rdfs:domain rdf:resource="#Observation"/>
    </owl:ObjectProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#usesStandard -->

    <owl:ObjectProperty rdf:about="#usesStandard">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="#Measurement"/>
        <rdfs:range rdf:resource="#MeasurementStandard"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property gives the standard (e.g., unit) used in a measurement.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasCode -->

    <owl:DatatypeProperty rdf:about="#hasCode">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;anySimpleType"/>
        <rdfs:comment
            >This property gives the code for a standard value.</rdfs:comment>
        <rdfs:domain rdf:resource="#StandardValue"/>
    </owl:DatatypeProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasMultiplier -->

    <owl:DatatypeProperty rdf:about="#hasMultiplier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:comment
            >This property gives the multiplier value of a unit conoboe. For example, to convert from celsius to fahrenheit we multiply by the multiplier 9/5 and add 32.</rdfs:comment>
        <rdfs:domain rdf:resource="#UnitConversion"/>
    </owl:DatatypeProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasOffset -->

    <owl:DatatypeProperty rdf:about="#hasOffset">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment
            >This property gives the offset value of a unit conoboe. For example, to convert from celsius to fahrenheit we multiply by 9/5 and add the offset 32.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:domain rdf:resource="#UnitConversion"/>
    </owl:DatatypeProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasPower -->

    <owl:DatatypeProperty rdf:about="#hasPower">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:comment
            >This propert gives the integer value that (base and simple) dimensions and units are raised to.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#BaseUnit"/>
                    <rdf:Description rdf:about="#SimpleDerivedUnit"/>
                    <rdf:Description rdf:about="#BaseDimension"/>
                    <rdf:Description rdf:about="#SimpleDerivedDimension"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasPrecision -->

    <owl:DatatypeProperty rdf:about="#hasPrecision">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property gives the precision of a measurement value.</rdfs:comment>
        <rdfs:domain rdf:resource="#Measurement"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#hasValue -->

    <owl:DatatypeProperty rdf:about="#hasValue">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;anySimpleType"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >This property gives the value of the measurement.</rdfs:comment>
        <rdfs:domain rdf:resource="#Measurement"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#BaseDimension -->

    <owl:Class rdf:about="#BaseDimension">
        <rdfs:subClassOf rdf:resource="#Dimension"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPower"/>
                <owl:hasValue rdf:datatype="&xsd;int">1</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A base dimension represents a base physical quantity, e.g., the SI base dimensions are length, mass, time, electric current, thermodynamic temperature, amount of substance, and luminous intensity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#BaseUnit -->

    <owl:Class rdf:about="#BaseUnit">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPower"/>
                <owl:hasValue rdf:datatype="&xsd;int">1</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#Unit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#forCharacteristic"/>
                <owl:allValuesFrom rdf:resource="#BaseDimension"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#SimpleDerivedUnit"/>
        <owl:disjointWith rdf:resource="#ComplexDerivedUnit"/>
        <rdfs:comment
            >A base unit represents an atomic unit from which other more complex units can be built.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Characteristic -->

    <owl:Class rdf:about="#Characteristic">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#characteristicOf"/>
                <owl:someValuesFrom rdf:resource="#Entity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCharacteristicValue"/>
                <owl:someValuesFrom rdf:resource="#CharacteristicValue"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#CharacteristicValue"/>
        <owl:disjointWith rdf:resource="#UnitConversion"/>
        <owl:disjointWith rdf:resource="#MeasurementStandard"/>
        <owl:disjointWith rdf:resource="#Entity"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <owl:disjointWith rdf:resource="#Measurement"/>
        <owl:disjointWith rdf:resource="#Context"/>
        <owl:disjointWith rdf:resource="#Observation"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A characteristic represents a property of an entity that can be measured (e.g., height, length, or color).  We adopt the distinction between things and properties of M. Bunge (1979). Namely, objects (entities) possess properties (characteristics), there are no objects without properties, and properties are attached to objects. When a characteristic is observed, it is measured, and a characteristic is the necessary focus of a measurement (i.e., a characteristic is a property of an entity being attributed a value by a measurement). Each characteristic is associated to exactly one entity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#CharacteristicValue -->

    <owl:Class rdf:about="#CharacteristicValue">
        <owl:disjointWith rdf:resource="#Measurement"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <owl:disjointWith rdf:resource="#UnitConversion"/>
        <owl:disjointWith rdf:resource="#Context"/>
        <owl:disjointWith rdf:resource="#MeasurementStandard"/>
        <owl:disjointWith rdf:resource="#Entity"/>
        <owl:disjointWith rdf:resource="#Observation"/>
        <rdfs:comment
            >A characteristic value represents a possible value for an entity characteristic.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#ComplexDerivedDimension -->

    <owl:Class rdf:about="#ComplexDerivedDimension">
        <rdfs:subClassOf rdf:resource="#Dimension"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasDimension"/>
                <owl11:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#BaseDimension"/>
                            <rdf:Description rdf:about="#SimpleDerivedDimension"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl11:onClass>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >A complex derived dimension is a derived physical quantity that consists of two or more base or simple physical quantities (dimensions).  Each component dimension is &quot;multiplied&quot; together to form the derived dimension. For example, a complex derived dimension representing acceleration  (length*time^-2) consists of the base dimension length and the simple derived dimension per time squared (time^-2).</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#ComplexDerivedUnit -->

    <owl:Class rdf:about="#ComplexDerivedUnit">
        <rdfs:subClassOf rdf:resource="#Unit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#forCharacteristic"/>
                <owl:allValuesFrom rdf:resource="#ComplexDerivedDimension"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasUnit"/>
                <owl11:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#SimpleDerivedUnit"/>
                            <rdf:Description rdf:about="#BaseUnit"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl11:onClass>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#SimpleDerivedUnit"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A complex derived unit is a unit that consists of two or more base or simple derived units.  Each component unit is &quot;multiplied&quot; together to form the derived unit. For example, a complex derived unit representing &apos;meter per second squared&apos; (m*s^-2) consists of the base unit &apos;meter&apos; (m) and the simple derived unit &apos;per second squared&apos; (s^-2).</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Context -->

    <owl:Class rdf:about="#Context">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasContextRelationship"/>
                <owl:someValuesFrom rdf:resource="#Relationship"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasContextObservation"/>
                <owl:someValuesFrom rdf:resource="#Observation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A context represents an asserted relationship between two observed entities at the time of observation. The relationship is directional in that one of the observed entities serves the role as context for the other.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#DateTime -->

    <owl:Class rdf:about="#DateTime">
        <rdfs:subClassOf rdf:resource="#IntervalRatioStandard"/>
        <owl:disjointWith rdf:resource="#NumericIndex"/>
        <owl:disjointWith rdf:resource="#Unit"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A date-time represents date and time values (e.g., 14:00, 2006-07-23).</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Dimension -->

    <owl:Class rdf:about="#Dimension">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ComplexDerivedDimension"/>
                    <rdf:Description rdf:about="#BaseDimension"/>
                    <rdf:Description rdf:about="#SimpleDerivedDimension"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#EntityClassification"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A dimension characteristic represents a physical quantity (both intensive and extensive) of an entity (e.g., length, weight, area, etc.)</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Entity -->

    <owl:Class rdf:about="#Entity">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCharacteristic"/>
                <owl:someValuesFrom rdf:resource="#Characteristic"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="#MeasurementStandard"/>
        <owl:disjointWith rdf:resource="#Measurement"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <owl:disjointWith rdf:resource="#Context"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >An entity denotes a concrete or conceptual object that has been observed (e.g., a tree, a community, an ecological process).  We adopt the distinction between objects and properties of M. Bunge (1979). Namely, objects (entities) possess properties (characteristics), there are no objects without properties, and properties are attached to objects.  Entities constitute the foci of observations, i.e., every observation is of some entity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#EntityClassification -->

    <owl:Class rdf:about="#EntityClassification">
        <rdfs:subClassOf rdf:resource="#Characteristic"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A classification characteristic is a directly measurable or recorded property (e.g., height, color, weight) of an entity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#EntityClassificationQualifier -->

    <owl:Class rdf:about="#EntityClassificationQualifier">
        <rdfs:subClassOf rdf:resource="#Characteristic"/>
        <owl:disjointWith rdf:resource="#EntityName"/>
        <owl:disjointWith rdf:resource="#EntityType"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >An Entity Classification Qualifier denotes a generic derived (e.g., computed) property of an entity.  These characteristics must be combined with classification characteristics (through intersection) to be used for measurement. For example, the qualifier Minimum can be combined with a (dimension) characteristic Length to create a measurable Minimum Length characteristic.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#EntityName -->

    <owl:Class rdf:about="#EntityName">
        <rdfs:subClassOf rdf:resource="#EntityClassification"/>
        <owl:disjointWith rdf:resource="#Dimension"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A name characteristic provides the name used for identifying (either globally or within a context) an entity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#EntityType -->

    <owl:Class rdf:about="#EntityType">
        <rdfs:subClassOf rdf:resource="#EntityClassification"/>
        <owl:disjointWith rdf:resource="#Dimension"/>
        <owl:disjointWith rdf:resource="#EntityName"/>
        <rdfs:isDefinedBy
            >A type characteristic provides the type (or class) of an entity as a value.</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#IntervalRatioStandard -->

    <owl:Class rdf:about="#IntervalRatioStandard">
        <rdfs:subClassOf rdf:resource="#OrdinalStandard"/>
        <rdfs:comment
            >An interval-ratio standard is an ordinal standard that further supports interval and ratio measurements. In an interval standard, equal differences between measurement values represent equivalent intervals in which differences between arbitrary pairs of measurements can be meaningfully compared. In a ratio standard, values additionally have meaningful ratios between arbitrary pairs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Measurement -->

    <owl:Class rdf:about="#Measurement">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ofCharacteristic"/>
                <owl:someValuesFrom rdf:resource="#Characteristic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasValue"/>
                <owl:someValuesFrom rdf:resource="&xsd;anySimpleType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#usesStandard"/>
                <owl:someValuesFrom rdf:resource="#MeasurementStandard"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPrecision"/>
                <owl:someValuesFrom rdf:resource="&xsd;anySimpleType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#MeasurementStandard"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <owl:disjointWith rdf:resource="#Context"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A measurement is an assertion that a characteristic of an entity was measured and/or recorded.  A measurement is also composed of a value, a measurement standard, and a precision (associated with the measured value).  Measurements also encapsulate characteristics that were recorded, but not necessarily measured in a physical sense.  For example, the name of a location and a taxon can be captured through measurements.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#MeasurementStandard -->

    <owl:Class rdf:about="#MeasurementStandard">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStandardValue"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#OrderedValue"/>
                            <rdf:Description rdf:about="#StandardValue"/>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Context"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A measurement standard defines a reference for comparing (or naming) entities. A measurement standard can be defined intentionally (e.g., as in the case of units) or extensionally (by listing the values of the standard, e.g., for color this might be red, blue, yellow, etc).</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#NominalStandard -->

    <owl:Class rdf:about="#NominalStandard">
        <rdfs:subClassOf rdf:resource="#MeasurementStandard"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A nominal standard is a measurement standard in which names are assigned to entities as labels.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#NumericIndex -->

    <owl:Class rdf:about="#NumericIndex">
        <rdfs:subClassOf rdf:resource="#IntervalRatioStandard"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A numeric index is typically defined by a mathematical equation, e.g., Shannon diversity, pH, and Richter scale are each numeric indices. Numeric indices typically go with characteristic qualifiers, e.g., the ratio of average height from two populations.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Observation -->

    <owl:Class rdf:about="#Observation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#ofEntity"/>
                <owl:someValuesFrom rdf:resource="#Entity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#MeasurementStandard"/>
        <owl:disjointWith rdf:resource="#Context"/>
        <owl:disjointWith rdf:resource="#Measurement"/>
        <owl:disjointWith rdf:resource="#Entity"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >An observation is an assertion that an entity was observed.  An observation may consist of measurements that refer to one or more measured characteristics of the entity.  Observations are made within a context. The context of an observation is given by other observations, e.g., an observation of a particular location may serve as context for an observation of an organism.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#OrderedValue -->

    <owl:Class rdf:about="#OrderedValue">
        <rdfs:subClassOf rdf:resource="#CharacteristicValue"/>
        <rdfs:comment
            >An ordered value is a characteristic value that is related to other characteristic values through a precedence (order) relation. Order is specified using the &apos;hasHigherRank&apos; property.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#OrdinalStandard -->

    <owl:Class rdf:about="#OrdinalStandard">
        <rdfs:subClassOf rdf:resource="#NominalStandard"/>
        <rdfs:comment
            >An ordinal standard is a nominal standard in which values represent a rank order (1st, 2nd, 3rd, etc.) of the entities measured.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Relationship -->

    <owl:Class rdf:about="#Relationship">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasRelationshipEntity"/>
                <owl:someValuesFrom rdf:resource="#Entity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Context"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A relationship object represents a directed relationship between a source and target entity.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#SimpleDerivedDimension -->

    <owl:Class rdf:about="#SimpleDerivedDimension">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasBaseDimension"/>
                <owl:someValuesFrom rdf:resource="#BaseDimension"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasPower"/>
                        <owl:hasValue rdf:datatype="&xsd;int">1</owl:hasValue>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasPower"/>
                        <owl:hasValue rdf:datatype="&xsd;int">0</owl:hasValue>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPower"/>
                <owl:someValuesFrom rdf:resource="&xsd;int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#Dimension"/>
        <rdfs:comment
            >A simple derived dimension represents a base physical quantity raised to a power (other than 0 or 1). For example, area is a simple derived dimension representing the base dimension length raised to the power 2.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#SimpleDerivedUnit -->

    <owl:Class rdf:about="#SimpleDerivedUnit">
        <rdfs:subClassOf rdf:resource="#Unit"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasPower"/>
                <owl:someValuesFrom rdf:resource="&xsd;int"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasPower"/>
                        <owl:hasValue rdf:datatype="&xsd;int">1</owl:hasValue>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#forCharacteristic"/>
                <owl:allValuesFrom rdf:resource="#SimpleDerivedDimension"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasBaseUnit"/>
                <owl:someValuesFrom rdf:resource="#BaseUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasPower"/>
                        <owl:hasValue rdf:datatype="&xsd;int">0</owl:hasValue>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A simple derived unit is a unit that raises a base unit to an integer power (other than 0 or 1).</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#StandardValue -->

    <owl:Class rdf:about="#StandardValue">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasCode"/>
                <owl:someValuesFrom rdf:resource="&xsd;anySimpleType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf rdf:resource="#CharacteristicValue"/>
        <rdfs:comment
            >A standard value is a specially designated characteristic value that is typically associated with a particular measurement standard.  All standard values have value codes that are used to denote the corresponding value within a measurement.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#Unit -->

    <owl:Class rdf:about="#Unit">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#BaseUnit"/>
                    <rdf:Description rdf:about="#SimpleDerivedUnit"/>
                    <rdf:Description rdf:about="#ComplexDerivedUnit"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#IntervalRatioStandard"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#forCharacteristic"/>
                <owl:allValuesFrom rdf:resource="#Dimension"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#NumericIndex"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >The base class for physical measurement units.</rdfs:comment>
    </owl:Class>
    


    <!-- http://code.ecoinformatics.org/code/semtools/trunk/dev/oboe/oboe.owl#UnitConversion -->

    <owl:Class rdf:about="#UnitConversion">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasSourceUnit"/>
                <owl:someValuesFrom rdf:resource="#BaseUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasTargetUnit"/>
                <owl:someValuesFrom rdf:resource="#BaseUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasMultiplier"/>
                <owl:someValuesFrom rdf:resource="&xsd;double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasOffset"/>
                <owl:someValuesFrom rdf:resource="&xsd;double"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Measurement"/>
        <owl:disjointWith rdf:resource="#Entity"/>
        <owl:disjointWith rdf:resource="#Context"/>
        <owl:disjointWith rdf:resource="#Observation"/>
        <owl:disjointWith rdf:resource="#MeasurementStandard"/>
        <owl:disjointWith rdf:resource="#Relationship"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >A unit conversion defines a mapping from a source unit to a target unit.</rdfs:comment>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="&rdf;List"/>
        <rdfs:subPropertyOf rdf:resource="#hasContext"/>
        <rdf:first rdf:resource="#hasObservationContext"/>
        <rdf:rest rdf:parseType="Collection">
            <rdf:Description rdf:about="#hasContextObservation"/>
        </rdf:rest>
    </rdf:Description>
</rdf:RDF>
